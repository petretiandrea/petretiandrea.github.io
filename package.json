{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve-hugo": "cd hugo-cv & hugo server -D",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "build-dev-hugo": "rm -rf ../public/cv/ & env-cmd -e development --use-shell \"cd hugo-cv && hugo -D -d ../public/cv/\"",
    "build-github-hugo": "rm -rf ../public/cv/ & env-cmd -e production --use-shell \"cd hugo-cv && hugo -D -d ../public/cv/\""
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@nuxt/devtools": "latest",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/i18n": "^9.0.0-rc.2",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/prismjs": "^1.26.3",
    "env-cmd": "^10.1.0",
    "nuxt": "^3.14.1592",
    "nuxt-icon": "^0.6.10",
    "nuxt-simple-sitemap": "^4.4.1",
    "puppeteer": "^23.10.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "dependencies": {
    "@giscus/vue": "^3.0.0",
    "@nuxt/content": "^2.13.4",
    "@nuxt/image": "^1.8.1",
    "clipboard": "^2.0.11",
    "nuxt-content-assets": "^1.4.4",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0"
  }
}
