{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-hugo": "cd hugo-cv & hugo server -D",
    "serve-all": "npm run build-dev-hugo & vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-all": "npm run build-github-hugo & vue-cli-service build",
    "build-dev-hugo": "rm -rf ../public/cv/ & env-cmd -e development --use-shell \"cd hugo-cv && hugo -D -d ../public/cv/\"",
    "build-github-hugo": "rm -rf ../public/cv/ & env-cmd -e production --use-shell \"cd hugo-cv && hugo -D -d ../public/cv/\"",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@codersrank/summary": "^0.9.11",
    "@mdi/font": "^6.3.95",
    "@mdi/js": "^5.9.55",
    "@yzfe/svgicon": "^1.0.1",
    "@yzfe/vue-svgicon": "^4.0.2",
    "core-js": "^3.18.1",
    "env-cmd": "^10.1.0",
    "html-loader": "^2.1.2",
    "puppeteer": "^10.4.0",
    "vue": "^2.6.14",
    "vue-custom-element": "^3.3.0",
    "vue-router": "^3.3.4",
    "vuetify": "^2.5.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@yzfe/svgicon-loader": "^1.0.5",
    "@yzfe/vue-cli-plugin-svgicon": "~1.0.4",
    "babel-eslint": "^10.1.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.17.0",
    "fibers": "^5.0.0",
    "sass": "^1.42.1",
    "sass-loader": "^9.0.3",
    "vue-cli-plugin-yaml": "^1.0.2",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
