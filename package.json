{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-hugo": "cd hugo-cv & hugo server -D",
    "serve-all": "npm run build-dev-hugo & vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-all": "npm run build-github-hugo & vue-cli-service build",
    "build-dev-hugo": "rm -rf ../public/cv/ & env-cmd -e development --use-shell \"cd hugo-cv && hugo -D -d ../public/cv/\"",
    "build-github-hugo": "rm -rf ../public/cv/ & env-cmd -e production --use-shell \"cd hugo-cv && hugo -D -d ../public/cv/\"",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@codersrank/summary": "^0.9.13",
    "@mdi/font": "^7.2.96",
    "@mdi/js": "^7.2.96",
    "@vue/compat": "^3.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@yzfe/svgicon": "^1.1.0",
    "@yzfe/vue-svgicon": "^4.0.3",
    "core-js": "^3.32.1",
    "env-cmd": "^10.1.0",
    "html-loader": "^4.2.0",
    "puppeteer": "^21.1.0",
    "roboto-fontface": "*",
    "vue": "^3.3.4",
    "vue-custom-element": "^3.3.0",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.14"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@yzfe/svgicon-loader": "^1.2.0",
    "@yzfe/vue-cli-plugin-svgicon": "~1.2.1",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "sass": "^1.66.1",
    "vite": "^4.2.0",
    "vite-plugin-vuetify": "^1.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
